{
  "name": "U-POWER-EVCCID",
  "version": "1.2.1",
  "scripts": {
    "start-dev": "NODE_ENV=development npx expo start",
    "start-dev-local": "NODE_ENV=development npx expo start --localhost",
    "start-staging": "NODE_ENV=test npx expo start",
    "start-staging-local": "NODE_ENV=test npx expo start --localhost",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "pod:install": "cd ios && pod install && cd ../",
    "pod:deintegrate": "cd ios && pod deintegrate && cd ../",
    "prebuild": "npx expo prebuild",
    "prebuild-clean": "npx expo prebuild --clean",
    "build-android-dev-local": "eas build --platform android --profile development --local",
    "build-ios-dev-local": "eas build --platform ios --profile development --local",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:build-aab": "cd android && ./gradlew bundleRelease && cd ../",
    "android:build-apk": "cd android && ./gradlew assembleRelease && cd ../"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/crashlytics": "^19.0.1",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "apisauce": "^3.0.1",
    "core-js": "^3.36.1",
    "dayjs": "^1.11.10",
    "expo": "^52.0.0",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-dev-client": "~5.0.11",
    "expo-file-system": "~18.0.10",
    "expo-sharing": "~13.0.1",
    "expo-status-bar": "~2.0.1",
    "jwt-decode": "^4.0.0",
    "patch-package": "^8.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "2.2.1",
    "react-native-vision-camera": "^4.6.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "jwt-decode",
          "core-js",
          "patch-package",
          "xlsx"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
