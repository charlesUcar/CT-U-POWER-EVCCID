{
  "name": "U-POWER-EVCCID",
  "version": "1.4.2",
  "scripts": {
    "start-dev": "NODE_ENV=development npx expo start",
    "start-dev-local": "NODE_ENV=development npx expo start --localhost",
    "start-staging": "NODE_ENV=staging npx expo start",
    "start-staging-local": "NODE_ENV=staging npx expo start --localhost",
    "start-prod": "NODE_ENV=production npx expo start",
    "start-prod-local": "NODE_ENV=production npx expo start --localhost",
    "android:staging": "EXPO_PUBLIC_APP_ENV=staging expo run:android",
    "android:dev": "EXPO_PUBLIC_APP_ENV=development expo run:android",
    "android": "EXPO_PUBLIC_APP_ENV=production expo run:android",
    "ios:staging": "EXPO_PUBLIC_APP_ENV=staging expo run:ios",
    "ios:dev": "EXPO_PUBLIC_APP_ENV=development expo run:ios",
    "ios": "EXPO_PUBLIC_APP_ENV=production expo run:ios",
    "web": "expo start --web",
    "pod:install": "cd ios && pod install && cd ../",
    "pod:deintegrate": "cd ios && pod deintegrate && cd ../",
    "prebuild": "npx expo prebuild",
    "prebuild-clean-dev": "EXPO_PUBLIC_APP_ENV=development npx expo prebuild --clean",
    "prebuild-clean-staging": "EXPO_PUBLIC_APP_ENV=staging npx expo prebuild --clean",
    "prebuild-clean-prod": "EXPO_PUBLIC_APP_ENV=production npx expo prebuild --clean",
    "build-android-dev-local": "eas build --platform android --profile development --local",
    "build-android-staging-local": "eas build --platform android --profile staging --local",
    "build-android-prod-local": "eas build --platform android --profile production --local",
    "build-ios-dev-local": "eas build --platform ios --profile development --local",
    "build-ios-staging-local": "eas build --platform ios --profile staging --local",
    "build-ios-prod-local": "eas build --platform ios --profile production --local",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:build-aab": "cd android && ./gradlew bundleRelease && cd ../",
    "android:build-apk": "cd android && ./gradlew assembleRelease && cd ../"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/config-plugins": "^54.0.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.5",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "apisauce": "^3.0.1",
    "babel-preset-expo": "^54.0.4",
    "core-js": "^3.36.1",
    "dayjs": "^1.11.10",
    "expo": "^54.0.13",
    "expo-asset": "~12.0.9",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.8",
    "expo-constants": "^18.0.10",
    "expo-dev-client": "~6.0.15",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "jwt-decode": "^4.0.0",
    "patch-package": "^8.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "2.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.17",
    "typescript": "~5.9.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "jwt-decode",
          "core-js",
          "patch-package",
          "xlsx"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
